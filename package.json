{
  "name": "vue-image-picker",
  "version": "1.0.0",
  "public": true,
  "license": "MIT",
  "description": "Easily select and upload images with this image picker component styled using Vuetify components!",
  "repositiory": {
    "type": "git",
    "url": "git+https://github.com/wallslide/vuetify-image-picker.git"
  },
  "bugs": {
    "url": "https://github.com/wallslide/vuetify-image-picker/issues"
  },
  "homepage": "https://github.com/wallslide/vuetify-image-picker#readme",
  "keywords": [
    "imagepicker",
    "vuetify",
    "image",
    "picker",
    "vue"
  ],
  "main": "./dist/vuetify-image-picker.common",
  "unpkg": "./dist/vuetify-image-picker.umd.min.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vuetify-image-picker ./src/main.js",
    "lint": "vue-cli-service lint",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "core-js": "2",
    "vue": "^2.6.10",
    "vuetify": "^1.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/vue": "^5.0.11",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5"
  }
}